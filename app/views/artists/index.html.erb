<div class="py-3 pt-8" style="padding-left: 92%;">
  <div class="py-1 px-2 btn flex flex-row-reverse items-center">
    <div><%= image_tag asset_path('yellowCircle.png')%></div>
    <h1 class="text-right font-extralight pr-3">LISTA</h1>
  </div>


</div>

<div class="flex flex-wrap justify-between">
  <% @artists.each do |artist|  %>
  <div class="randomize" style="max-height: 28%;  max-width: 28%; padding-bottom: 1%;">
    <p class="uppercase text-2xl font-light py-10"><%= link_to "#{artist.name}", artist_path(artist)  %></p>
    <div><%= image_tag "#{artist.about}" %></div>
  </div>
  <% end %>
</div>





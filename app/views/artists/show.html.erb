<div class="uppercase text-8xl font-thin pl-12" style="padding-top: 4%; padding-bottom: 6%;"><%= @artist.name %></div>


<!-- swiper gem -->
<div class="swiper">
  <!-- Additional required wrapper -->
  <div class="swiper-wrapper">
    <!-- Slides -->
    <%@artist.photos.each  do |photo|  %>
      <div class="swiper-slide">
        <%= image_tag 'photo' %>
      </div>
    <% end %>
  </div>

  <!-- If we need pagination -->
  <!-- div class="swiper-pagination"></!-->

  <!-- If we need navigation buttons -->
  <div class="swiper-button-prev"></div>
  <div class="swiper-button-next"></div>
</div>

<!-- placeholder de carrousel de fotos -->
<!-- averiguar como hacer para que esta seccion (el carrousel) no herede los margenes, como hacer para que quede fuera de la regla de margenes de todo el resto -->

  <div class="logo-navbar pt-1 pb-10" style="padding-bottom: 9%;">
    <%= image_tag asset_path('carrousel.png')%>
  </div>



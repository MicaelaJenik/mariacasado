<!DOCTYPE html>
<html>
  <head>
    <title>Mariacasado</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>

    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload', defer: true %>
  </head>

  <body>
  <div class="px-16" style="font-family: menlo;">
    <div class="flex py-4">
      <h1 class="flex-grow text-center"><%= link_to "artistas", artists_path %></h1>
      <h1 class="flex-grow text-center"><%= link_to "nosotros", press_path %></h1>
      <h1 class="flex-grow text-center"><%= link_to "muestras", shows_path %></h1>
      <h1 class="flex-grow text-center"><%= link_to "ferias", root_path %></h1>
      <h1 class="flex-grow text-center"><%= link_to "random", press_path %></h1>
      <h1 class="flex-grow text-center"><%= link_to "playlist", press_path %></h1>
      <h1 class="flex-grow text-center"><%= link_to "contacto", press_path %></h1>
    </div>

    <div class="flex pb-14">
      <div class="flex-grow"><%= image_tag asset_path('logo.png')%></div>
      <div class="flex-grow text-right">lista</div>
    </div>

    <%= yield %>

  </div>




  </body>
</html>


